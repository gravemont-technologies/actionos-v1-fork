{
  "name": "action-os",
  "version": "0.1.0",
  "description": "Action-First OS MVP delivering strategic guidance with feedback loop.",
  "type": "module",
  "main": "dist/server/index.js",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "tsx src/server/index.ts",
    "dev:client": "vite",
    "test:smoke": "node scripts/api-smoke.mjs http://localhost:3001",
    "test:openai": "node scripts/test-openai-key.mjs",
    "logs:pct": "node scripts/aggregate-logs.mjs",
    "perf:client": "echo \"Run: npx --yes @lhci/cli@0.13.0 autorun --collect.url=http://localhost:3000 --collect.url=http://localhost:3000/app/analyze\"",
    "load:server": "echo \"Run: k6 run -e BASE_URL=http://localhost:3001 scripts/k6-analyze.js && k6 run -e BASE_URL=http://localhost:3001 -e PROFILE_ID=<id> -e SIGNATURE=<hex> -e USER_ID=<user> scripts/k6-feedback.js\"",
    "test": "vitest run",
    "test:unit": "vitest run tests --reporter=default",
    "test:ui": "vitest run tests --environment=jsdom --reporter=default",
    "test:api": "vitest run tests/api --reporter=default",
    "test:integration": "vitest run tests/integration --reporter=default",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "build": "npm run build:server && npm run build:client",
    "build:server": "tsc -p tsconfig.json",
    "build:client": "vite build",
    "start": "node dist/server/index.js",
    "start:prod": "NODE_ENV=production node dist/server/index.js",
    "lint": "tsc --noEmit",
    "lint:fix": "tsc --noEmit --pretty",
    "preview": "vite preview",
    "precommit": "npm run lint",
    "test:build": "npm run build && echo 'Build successful'",
    "test:types": "npm run lint && echo 'Type check passed'",
    "health": "curl http://localhost:3001/api/health || echo 'Server not running'",
    "db:apply-schema": "node scripts/apply-schema.mjs"
    ,
    "test:feedback": "tsx scripts/test-feedback-route.ts"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@clerk/backend": "^1.20.0",
    "@clerk/clerk-react": "^5.55.0",
    "@hookform/resolvers": "^3.10.0",
    "@radix-ui/react-accordion": "^1.2.11",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-aspect-ratio": "^1.1.7",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-context-menu": "^2.2.15",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-hover-card": "^1.1.14",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-menubar": "^1.1.15",
    "@radix-ui/react-navigation-menu": "^1.2.13",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.7",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toast": "^1.2.14",
    "@radix-ui/react-toggle": "^1.1.9",
    "@radix-ui/react-toggle-group": "^1.1.10",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@supabase/supabase-js": "^2.81.1",
    "@tanstack/react-query": "^5.83.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^3.6.0",
    "dotenv": "^17.2.3",
    "embla-carousel-react": "^8.6.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.4.1",
    "html2canvas": "^1.4.1",
    "input-otp": "^1.4.2",
    "jspdf": "^3.0.3",
    "lucide-react": "^0.462.0",
    "next-themes": "^0.4.6",
    "pino": "^9.5.0",
    "pino-pretty": "^13.0.0",
    "react": "^19.2.0",
    "react-day-picker": "^9.11.1",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.61.1",
    "react-resizable-panels": "^2.1.9",
    "react-router-dom": "^6.30.1",
    "recharts": "^2.15.4",
    "sonner": "^1.7.4",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^1.1.2",
    "web-vitals": "^4.2.4",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.19",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@types/express": "^5.0.5",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.4",
    "@types/react-dom": "^19.2.3",
    "@types/supertest": "^6.0.3",
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "jsdom": "^25.0.1",
    "postcss": "^8.5.6",
    "supertest": "^7.1.4",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vite": "^7.2.2",
    "vitest": "^2.1.3"
  },
  "overrides": {
    "esbuild": "^0.25.0"
  }
}
